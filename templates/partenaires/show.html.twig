{% extends 'base.html.twig' %}

{% block title %}Partenaires{% endblock %}

{% block body %}
    <h1>Partenaires</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ partenaire.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ partenaire.nom }}</td>
            </tr>
            <tr>
                <th>Active</th>
                <td>{{ partenaire.active ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ partenaire.email }}</td>
            </tr>
            <tr>
                <th>Mot_de_passe</th>
                <td>{{ partenaire.motDePasse }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_partenaires_index') }}">back to list</a>

    <a href="{{ path('app_partenaires_edit', {'id': partenaire.id}) }}">edit</a>

    {{ include('partenaires/_delete_form.html.twig') }}
{% endblock %}
